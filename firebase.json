{
  "hosting": {
    "public": "dist/app-server",
    "main": "main.js",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "ssr"
      }
    ]
  },
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR/..\" run lint",
      "npm --prefix \"$RESOURCE_DIR/..\" run build:ssr && npm --prefix \"$RESOURCE_DIR\" run build"
    ]
  }
}
